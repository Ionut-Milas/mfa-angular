{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "common": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/common",
      "sourceRoot": "projects/common/src",
      "prefix": "common",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/common",
            "index": "projects/common/src/index.html",
            "main": "projects/common/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/common/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/common/src/favicon.ico",
              "projects/common/src/assets"
            ],
            "styles": [
              "projects/common/src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "projects/common/webpack.config.dev.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "projects/common/webpack.config.prod.ts"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "common:build:production"
            },
            "development": {
              "browserTarget": "common:build:development",
              "port": 4203
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "common:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/common/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/common/src/favicon.ico",
              "projects/common/src/assets"
            ],
            "styles": [
              "projects/common/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
